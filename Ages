#Домашнее задание №1
#Условный оператор: Возраст
print('Сколько вам лет?')
age = int(input())
def how_old_are_u(age):
    if age <5:
        return('Вам надо учиться в детском саду')
    elif 5<= age <17:
        return('Вам надо учиться в школе')
    elif 17<= age <21:
        return('Вам надо учиться в университете')
    elif 21<= age <65:
        return('Вам надо работать')
    elif  age > 65:
        return('Чаще путешествуйте')
    
age = how_old_are_u(age)
print(age)